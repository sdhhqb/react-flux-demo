{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            1, 
            {"vars": "all", "args": "after-used"}
        ],
        "no-console": 0
    },
    "env": {
        "browser": true
    },
    "globals": {
        "require": true,
        "module": true,
        "$": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}